{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:jest/recommended",
    "jest-enzyme"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-use-before-define": 0,

    "import/no-unresolved": [
      0,
      { "ignore": ["^components"] }
    ],
  }
}
