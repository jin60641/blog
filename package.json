{
  "name": "jinsang-blog",
  "private": true,
  "description": "Jinsang's blog by Gatsby",
  "version": "0.1.0",
  "author": "JinSangHyeon <jin60641@gmail.com>",
  "dependencies": {
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-resolve-src": "^2.0.0-beta.1",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.1.8",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "posts": "git submodule update --init --recursive --remote",
    "deploy": "npm run posts && npm run build && gh-pages -d public -b gh-pages",
    "start": "npm run develop",
    "test": "jest --no-cache",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint -c .eslintrc 'src/**/*.js*'",
    "lint:diff": "git diff-index --name-only --diff-filter=dbx HEAD | grep -E \"(.*)\\.(jsx|js)$\" | xargs eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-gatsby": "^0.1.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-jest-enzyme": "^7.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "npm run lint:diff"
    }
  }
}
